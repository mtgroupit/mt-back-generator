package api  {{$modelName := .CurModel}} {{$model := (index $.Models $modelName)}}

import (
	"testing"

	"{{.Module}}/internal/api/restapi/client"
	{{LowerTitle $modelName}} "{{.Module}}/internal/api/restapi/client/{{NameSQL $modelName}}"
	
	cl "github.com/go-openapi/runtime/client"
	"github.com/golang/mock/gomock"
	"github.com/powerman/check"
)

{{range $i, $method := $model.Methods}}{{if not (IsCustomMethod $method)}}
func Test{{$method}}{{$modelName}}(tt *testing.T) {
	t := check.T(tt)
	// t.Parallel()
	tsURL, shutdown, mockApp, {{if not (index $model.MethodsProps $i).NoSecure}}mockExtAuthSvc{{else}}_{{end}} := testNewServer(t)
	defer shutdown()
	c := client.NewHTTPClientWithConfig(nil, &client.TransportConfig{
		Host: tsURL,
	})

	{{- if not (index $model.MethodsProps $i).NoSecure}}
		mockExtAuthSvc.EXPECT().GetUserProfile(gomock.Any(), matchCookie(sess)).Return(profile, nil)
	{{- end}}

	params := {{LowerTitle $modelName}}.New{{$method}}{{$modelName}}Params()
	{{- if IsGet $method}}
		{{- if not (IsMyMethod $method)}}
			params.Body.ID = test{{$modelName}}ID1
		{{- end}}
		mockApp.EXPECT().{{$method}}{{$modelName}}(gomock.Any(){{if not (IsMyMethod $method)}}, gomock.Any(){{end}}).Return(app{{$modelName}}(test{{$modelName}}1{{if $model.HaveLazyLoading}}, true{{end}}), nil)

		t.Run("", func(tt *testing.T) {
			t := check.T(tt)
			geted, err := c.{{$model.TitleName}}.{{$method}}{{$modelName}}(params{{if not (index $model.MethodsProps $i).NoSecure}}, cl.APIKeyAuth("Cookie", "header", sess){{end}})
			t.Nil(err)
			t.DeepEqual(geted.Payload, api{{$modelName}}(app{{$modelName}}(test{{$modelName}}1{{if $model.HaveLazyLoading}}, true{{end}})))
		})
	{{- end}}
	{{- if IsAdd $method}}	
		params.Body = testAdd{{$modelName}}1
		mockApp.EXPECT().{{$method}}{{$modelName}}(gomock.Any(), gomock.Any()).Return(app{{$modelName}}(test{{$modelName}}1{{if $model.HaveLazyLoading}}, true{{end}}), nil)

		t.Run("", func(tt *testing.T) {
			t := check.T(tt)
			geted, err := c.{{$model.TitleName}}.{{$method}}{{$modelName}}(params{{if not (index $model.MethodsProps $i).NoSecure}}, cl.APIKeyAuth("Cookie", "header", sess){{end}})
			t.Nil(err)
			t.DeepEqual(geted.Payload, api{{$modelName}}(app{{$modelName}}(test{{$modelName}}1{{if $model.HaveLazyLoading}}, true{{end}})))
		})
	{{- end}}
	{{- if IsDelete $method}}
		{{- if not (IsMyMethod $method)}}
			params.Body.ID = test{{$modelName}}1.ID
		{{- end}}
		mockApp.EXPECT().{{$method}}{{$modelName}}(gomock.Any(){{if not (IsMyMethod $method)}}, gomock.Any(){{end}}).Return(nil)

		t.Run("", func(tt *testing.T) {
			t := check.T(tt)
			_, err := c.{{$model.TitleName}}.{{$method}}{{$modelName}}(params{{if not (index $model.MethodsProps $i).NoSecure}}, cl.APIKeyAuth("Cookie", "header", sess){{end}})
			t.Nil(err)
		})
	{{- end}}
	{{- if IsEdit $method}}
		{{- if IsAdjustEdit $method}}
			params.Body = {{LowerTitle $modelName}}.{{$method}}{{$model.TitleName}}Body{
				{{- if not (IsMyMethod $method)}}
					ID: test{{$modelName}}1.ID,
				{{- end}}
				Data: &{{LowerTitle $modelName}}.{{$method}}{{$model.TitleName}}ParamsBodyData{
					{{- range $name, $options := $model.Columns}}
						{{- if ContainsStr (index $model.MethodsProps $i).EditableFields $name}}
							{{$options.TitleName}}: test{{$modelName}}1.{{$options.TitleName}}{{if $options.IsStruct}}.ID{{end}},
						{{- end}}
					{{- end}}
				},
			}
			{{- if $model.ReturnWhenEdit}}
				test{{$modelName}}1Edited := test{{$modelName}}1
				{{- range $name, $options := $model.Columns}}
					{{- if ContainsStr (index $model.MethodsProps $i).EditableFields $name}}
						test{{$modelName}}1Edited.{{$options.TitleName}}{{if $options.IsStruct}}.ID{{end}} = test{{$modelName}}1.{{$options.TitleName}}{{if $options.IsStruct}}.ID{{end}}
					{{- end}}
				{{- end}}
			{{- end}}
		{{- else}}
			params.Body.Data = testAdd{{$modelName}}1
			{{- if not (IsMyMethod $method)}}
				params.Body.ID = test{{$modelName}}1.ID
			{{- end}}
		{{- end}}
		mockApp.EXPECT().{{$method}}{{$modelName}}(gomock.Any(), {{if not (IsMyMethod $method)}}gomock.Any(), {{end}}gomock.Any()).Return({{if $model.ReturnWhenEdit}}app{{$modelName}}(test{{$modelName}}1{{if IsAdjustEdit $method}}Edited{{end}}{{if $model.HaveLazyLoading}}, true{{end}}), {{end}}nil)
		t.Run("", func(tt *testing.T) {
			t := check.T(tt)
			{{if $model.ReturnWhenEdit}}geted{{else}}_{{end}}, err := c.{{$model.TitleName}}.{{$method}}{{$modelName}}(params, cl.APIKeyAuth("Cookie", "header", sess))
			t.Nil(err)
			{{- if $model.ReturnWhenEdit}}
				t.DeepEqual(geted.Payload, api{{$modelName}}(app{{$modelName}}(test{{$modelName}}1{{if IsAdjustEdit $method}}Edited{{end}}{{if $model.HaveLazyLoading}}, true{{end}})))
			{{- end}}
		})
	{{- end}}
	{{- if IsList $method}}
		params.Body = testList
		mockApp.EXPECT().{{$method}}{{$modelName}}(gomock.Any(), gomock.Any()).Return(app{{$modelName}}s(test{{$modelName}}s{{if $model.HaveLazyLoading}}, true{{end}}), {{if $model.DetailedPagination}}2, {{end}}[]string{}, nil)

		t.Run("", func(tt *testing.T) {
			t := check.T(tt)
			list, err := c.{{$model.TitleName}}.{{$method}}{{$modelName}}(params{{if not (index $model.MethodsProps $i).NoSecure}}, cl.APIKeyAuth("Cookie", "header", sess){{end}})
			t.Nil(err)
			t.DeepEqual(list.Payload.Items, api{{$modelName}}s(app{{$modelName}}s(test{{$modelName}}s{{if $model.HaveLazyLoading}}, true{{end}})))
		})
	{{- end}}
}
{{end}}{{end}}